<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>servlet.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Abastecimento</servlet-name>
        <servlet-class>servlet.Abastecimento</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Fornecimento</servlet-name>
        <servlet-class>servlet.Fornecimento</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ajaxQuantia</servlet-name>
        <servlet-class>servlet.ajaxQuantia</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Registro</servlet-name>
        <servlet-class>servlet.Registro</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Registro</servlet-name>
        <url-pattern>/Registro</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ajaxCidade</servlet-name>
        <servlet-class>servlet.ajaxCidade</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ajaxCidade</servlet-name>
        <url-pattern>/ajaxCidade</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Abastecimento</servlet-name>
        <url-pattern>/Abastecimento</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Fornecimento</servlet-name>
        <url-pattern>/Fornecimento</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ajaxQuantia</servlet-name>
        <url-pattern>/ajaxQuantia</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
